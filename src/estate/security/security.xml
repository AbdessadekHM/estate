<odoo>
    <record id="module_category_real_estate_brokerage" model="ir.module.category">

        <field name="name">Real Estate Brokerage</field>
        <field name="description">Real Estate Brokerage Desc</field>
        <field name="sequence">10</field>

    </record>

    <record id="module_category_real_estate_brokerage_privilege" model="res.groups.privilege">

        <field name="name">Real Estate Brokerage privilege</field>
        <field name="category_id" ref="estate.module_category_real_estate_brokerage"/>

    </record>

    <record id="estate_group_users" model="res.groups">
        <field name="name">Users</field>
        <field name="privilege_id" ref="estate.module_category_real_estate_brokerage_privilege"/>

    </record>


    <record id="estate_group_managers" model="res.groups">
        <field name="name">Managers</field>
        <field name="privilege_id" ref="estate.module_category_real_estate_brokerage_privilege"/>
        <field name="implied_ids" eval="[(4,ref('estate.estate_group_users'))]"/>

    </record>


    
</odoo>
