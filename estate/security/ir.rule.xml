<odoo>

    <record id="estate_managers_estate_property_rule" model="ir.rule">
        <field name="name">estate.managers.estate.property.rule</field>
        <field name="model_id" ref="estate.model_estate_property"/>
        <field name="domain_force">[(1, '=' , 1)]</field>

        <field name="groups" eval="[(4,ref('estate.estate_group_managers'))]"/>

    </record>

    <record id="estate_managers_estate_property_tag_rule" model="ir.rule">
        <field name="name">estate.managers.estate.property.tag.rule</field>
        <field name="model_id" ref="estate.model_estate_property_tag"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="groups" eval="[(4,ref('estate.estate_group_managers'))]"/>

    </record>

    <record id="estate_managers_estate_property_type_rule" model="ir.rule">
        <field name="name">estate.managers.estate.property.type.rule</field>
        <field name="model_id" ref="estate.model_estate_property_type"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="groups" eval="[(4,ref('estate.estate_group_managers'))]"/>

    </record>

    <record id="estate_managers_estate_property_offer_rule" model="ir.rule">
        <field name="name">estate.managers.estate.property.offer.rule</field>
        <field name="model_id" ref="estate.model_estate_property_offer"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="groups" eval="[(4,ref('estate.estate_group_managers'))]"/>

    </record>











    <record id="estate_users_estate_property_rule" model="ir.rule">
        <field name="name">estate.users.estate.property.rule</field>
        <field name="model_id" ref="estate.model_estate_property"/>
        <field name="domain_force">[('create_uid','=',user.id)]</field>
        <field name="groups" eval="[(4,ref('estate.estate_group_users'))]"/>

    </record>

</odoo>
