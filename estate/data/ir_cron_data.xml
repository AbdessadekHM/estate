<odoo>
    <record id="estate_cron_action" model="ir.cron">
        <field name="name">Estate Cron Action</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <!-- <field name="interval_type">minutes</field> -->
        <field name="model_id" ref="model_estate_property" />
        <field name="nextcall" eval="(datetime.now() + relativedelta(days=1)).strftime('%Y-%m-%d 15:00:00')" />
        <field name="state">code</field>
        <field name="code">model._cron_cancel_action()</field>


    </record>
</odoo>